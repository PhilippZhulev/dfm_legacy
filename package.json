{
  "name": "dfm_it",
  "version": "2.0.31",
  "description": "DFM IT",
  "developer": "Heavy mouse team",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "env:example": "cross-env SOURCE=src-example",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/*.js src/**/*.js src/**/**/*.js  src/**/**/**/*.js",
    "prettier:lib": "prettier --write src/*.js src/lib/*.js",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint --debug src/",
    "storybook": "start-storybook -p 9001 -c private/storybook -s ./public",
    "storybook:example": "npm run env:example -- npm run storybook",
    "dev-docker": "npm run env:dev -- webpack-dev-server --inline --progress --host 0.0.0.0",
    "dev": "npm run env:dev -- webpack-dev-server",
    "dev:example": "npm run env:example -- npm run dev",
    "build:clean": "rimraf \"build/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* build",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "node ./appver -f package.json -k version && npm run env:prod -- webpack",
    "build:example": "npm run env:example -- npm run build",
    "build:env": "env-cmd -f .env.${REACT_APP_ENV} npm run build",
    "build:prod": "REACT_APP_ENV=production npm run build:env",
    "build:staging": "REACT_APP_ENV=staging npm run build:env",
    "git:commit:feature": "git add . && git commit -am \"updated\" && git push origin origin/feature/model_screen",
    "git:commit:master": "git add . && git commit -am \"updated\" && git push --force origin master",
    "git:commit:develop": "git add . && git commit -am \"updated\" && git push --force origin origin/develop",
    "git:commit:fix": "git add . && git commit -am \"updated\" && git push --force origin origin/hotfixes",
    "git:push:feature": "git push --force origin origin/feature/$BRANCH",
    "git:push:master": "git push --force origin master",
    "git:push:develop": "git push --force origin origin/develop",
    "git:push:fix": "git push --force origin origin/hotfixes",
    "git:pull:develop": "git checkout origin origin/develop && git pull --rebase origin",
    "git:pull:master": "git checkout master && git pull --rebase origin",
    "git:merge:develop": "git merge origin origin/develop",
    "git:merge:master": "git merge origin master",
    "download-svg": "node ./download-svg.js"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^(components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/setupTests.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "npm run test",
      "git add"
    ]
  },
